<br><br>
<div style="text-align: center;" id="clearance" class="sign-up">
  <h2><%= t(".title") %></h2>
<br>
  <%= form_for @user do |form| %>
    <%= render partial: '/users/form', object: form %>
<br>
    <div class="submit-field">
      <%= form.submit %>
    </div>
<br>
    <div class="other-links">
      <%= link_to t(".sign_in"), sign_in_path %>
    </div>
  <% end %>
</div>
